{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "storage",
      "type": "blob",
      "path": "{username}/{filename}",
      "direction": "out",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "scriptFile": "../dist/robotica-picture-validation/index.js"
}
